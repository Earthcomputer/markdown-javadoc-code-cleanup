<idea-plugin>
    <id>net.earthcomputer.markdownjavadoccodecleanup</id>
    <name>Markdown Javadoc Code Cleanup</name>
    <vendor>Earthcomputer</vendor>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.java</depends>

    <resource-bundle>messages.MyBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <localInspection
                cleanupTool="true"
                language="JAVA"
                bundle="messages.MyBundle"
                key="inspection.markdown.javadoc.code.cleanup.display.name"
                groupBundle="messages.InspectionsBundle"
                groupKey="group.names.language.level.specific.issues.and.migration.aids23"
                groupPathKey="group.path.names.java.language.level.specific.issues.and.migration.aids"
                enabledByDefault="false"
                level="WARNING"
                implementationClass="net.earthcomputer.markdownjavadoccodecleanup.MarkdownJavadocCodeCleanupInspection"/>
    </extensions>
</idea-plugin>
